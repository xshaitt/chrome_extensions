{
  "manifest_version": 2,
  "name": "帅的天气查看器",
  "version": "0.1",
  "description": "帅的天气查看器",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "天气查看器",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "http://www.weather.com.cn/data/sk/101010100.html"
  ]
}